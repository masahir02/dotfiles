#!/usr/bin/env zsh

brew update
brew upgrade
brew bundle --global
brew cleanup --prune=all
brew bundle cleanup --force --global

gibo update

gcloud components update --quiet

asdf plugin-update --all

source ~/.zshrc
zinit self-update
zinit update --all

vim +PlugUpgrade +PlugInstall +PlugUpdate +PlugClean! +CocUpdateSync +qa
